              },
              "description": {
                "type": "string",
                "description": "电影描述"
              },
              "language": {
                "type": "array",
                "description": "电影语言",
                "items": {
                  "type":"string",
                  "description": "语言id"
                },
                "required": true
              },
              "author_rate": {
                "type": "integer",
                "description": "评分"
              },
              "alias": {
                "type": "array",
                "description": "电影别名",
                "items": {
                  "type":"string"
                }
              },
              "author_description": {
                "type": "string",
                "description": "作者对电影的描述"
              }
